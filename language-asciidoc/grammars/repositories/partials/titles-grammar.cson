key: 'titles'

patterns: [

  # Matches section titles (headers)
  #
  # Examples
  #
  #   = Document Title (Level 0)
  #
  #   == Level 1 Section
  #
  #   === Level 2 Section
  #
  name: 'markup.heading.asciidoc'
  begin: '^((?:=|#){1,6})([ \\t]+)(?=\\S+)'
  patterns: [
    include: '$self'
  ]
  end: '$'
]
